    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000001e2d880ac10_0;
    %parti/s 3, 5, 4;
    %pushi/vec4 0, 0, 3;
    %and;
    %inv;
    %ix/load 4, 5, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v000001e2d8807050_0, 4, 5;
T_0.1 ;
    %load/vec4 v000001e2d880ac10_0;
    %parti/s 6, 2, 3;
    %pad/u 5;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v000001e2d8807050_0, 4, 5;
    %jmp T_0;
